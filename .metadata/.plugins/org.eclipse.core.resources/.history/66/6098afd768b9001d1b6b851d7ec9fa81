package base.comp;

import javax.swing.Icon;
import javax.swing.JLabel;

public class imageLable extends JLabel {

	public imageLable() {
		// TODO Auto-generated constructor stub
	}

	public imageLable(String text) {
		super(text);
		// TODO Auto-generated constructor stub
	}

	public imageLable(Icon image) {
		super(image);
		// TODO Auto-generated constructor stub
	}

	public imageLable(String text, int horizontalAlignment) {
		super(text, horizontalAlignment);
		// TODO Auto-generated constructor stub
	}

	public imageLable(Icon image, int horizontalAlignment) {
		super(image, horizontalAlignment);
		// TODO Auto-generated constructor stub
	}

	public imageLable(String text, Icon icon, int horizontalAlignment) {
		super(text, icon, horizontalAlignment);
		// TODO Auto-generated constructor stub
	}

}
